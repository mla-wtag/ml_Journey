<h1>Login</h1>
<%= form_with model: @user, url: user_sessions_path do |f| %>
  <div>
    <%= f.label :firstname %><br>
    <%= f.text_field :firstname, placeholder: "Firstname" %>
  </div>
  <div>
    <%= f.label :password %><br>
    <%= f.text_field :password, placeholder: "Password" %>
  </div>
  <p>
    <%= f.submit "Login" %>
  </p>
<% end %>
<%= link_to "Register", new_user_path %>

<h1>All Users</h1>
<%= link_to 'New User', new_user_path %>
<table>
  <thead>
    <tr>
      <th>id</th>
      <th>firstname</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.firstname %></td>
      </tr>
    <% end %>
  </tbody>
</table>
